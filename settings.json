{
    "code-runner.runInTerminal": true,
    //"workbench.colorTheme": "One Dark Pro Flat",
    "editor.mouseWheelZoom": true,
    "files.autoSave": "afterDelay",
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "workbench.iconTheme": "vscode-icons",
    "gulp.autoDetect": "on",
    "files.autoGuessEncoding": true,
    "workbench.sideBar.location": "right"

    "background.useDefault": false,  // 是否使用默认图片，改成false不默认，设 置自己想要的
    "background.style": {
        "content": "''",
        "pointer-events": "none",
        "position": "absolute",//定位
        "bottom": "0px",
        "right": "0",
        "z-index": "99999",
        "width": "100%",
        "height": "100%",
        "background-position": "0% 0%",
        "background-size": "cover", //图片大小
        "background-repeat": "no-repeat",//是否重复
        "opacity": 0.3 //透明度
    },
    "background.customImages": ["file:///D:\\material\\peakyblinky\\1-4.jpg"]//图片的路径地址 ,最多3张

    {
      // background相关配置 Start
      "background.useDefault": false,
      "background.enabled": true,
      "background.style": {
        "content": "''",
        "pointer-events": "none",
        "position": "absolute",
        "bottom": "0px",
        "right": "0",
        "z-index": "99999",
        "width": "100%",
        "height": "100%",
        "background-position": "0% 0%",
        "background-size": "cover",
        "background-repeat": "no-repeat",
        "opacity": 0.3
      },

      "workbench.colorTheme": "Solarized Dark",
      "explorer.confirmDragAndDrop": false,
      "editor.formatOnSave": true,
      "editor.fontSize": 17,
    }
}

